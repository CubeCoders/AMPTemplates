[
    {
        "ConfigFile": "RDS_Data/config/rds_config.ini",
        "ConfigType": "ini",
        "ConfigFormat": "{0} = {1}",
        "Subsections": [
            {
                "Heading": "Authentication",
                "SettingMappings": {
                    "MasterPrivateKey": "MasterPrivateKey",
                    "SteamAPIKey": "SteamAPIKey"
                }
            },
            {
                "Heading": "General",
                "SettingMappings": {
                    "ServerName": "ServerName",
                    "ServerPassword": "ServerPassword",
                    "MaxPlayers": "$MaxUsers",
                    "PVP": "PVP",
                    "IconUrl": "IconUrl",
                    "BannerUrl": "BannerUrl",
                    "ShownInServerlist": "ShownInServerlist"
                }
            },
            {
                "Heading": "Networking",
                "SettingMappings": {
                    "NetworkingLayer": "NetworkingLayer",
                    "ConsolePort": "$ConsolePort",
                    "RTCPPort": "$RTCPPort"
                }
            }
        ]
    }
]