[
    {
        "ConfigFile": "server.cfg",
        "ConfigType": "kvp",
        "ConfigFormat": "{0} = {1};",
        "Subsections": [
            {
                "Heading": "$root",
                "SettingMappings": {
                    "hostname": "ServerName",
                    "passwordAdmin": "AdminPassword",
                    "password:": "ServerPassword",
                    "motd[]": "MOTD",
                    "motdInterval": "MOTDInterval",
                    "maxPlayers": "$MaxUsers",
                    "disableVoN": "DisableVoN",
                    "vonCodecQuality": "VoiceQuality",
                    "voteMissionPlayers": "VoteMissionPlayers",
                    "voteThreshold": "VoteThreshold",
                    "persistent": "EnablePersistent",
                    "BattlEye": "BattlEye",
                    "verifySignatures": "VerifySignatures",
                    "kickDuplicate": "KickDuplicate",
                    "timeStampFormat": "TimeStampFormat",
                    "logFile": "LogFile"
                }
            }
        ]
    },
    {
        "ConfigFile": "battleye/beserver.cfg",
        "ConfigType": "kvp",
        "ConfigFormat": "{0} {1}",
        "Subsections": [
            {
                "Heading": "$root",
                "SettingMappings": {
                    "RConPassword" : "$RemoteAdminPassword",
                    "RConPort" : "$RemoteAdminPort",
                    "RConIP" : "RemoteAdminIP"
                }
            }
        ]
    },
    {
        "ConfigFile": "battleye/beserver_x64.cfg",
