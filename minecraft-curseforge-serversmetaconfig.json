[
    {
        "ConfigFile": "server.properties",
        "AutoMap": false,
        "Importable": true,
        "ConfigType": "kvp",
        "SettingMappings": {
            "motd=": "ServerName",
            "max-players=": "MaxPlayers",
            "enable-rcon=": "EnableRcon",
            "rcon.password=": "RconPassword",
            "enable-query=": "EnableQuery",
            "query.port=": "$QueryPort",
            "server-port=": "$GamePort",
            "rcon.port=": "$RCONPort"
        }
    },
    {
        "ConfigFile": "eula.txt",
        "AutoMap": false,
        "Importable": true,
        "ConfigType": "kvp",
        "ConfigFormat": "{0}={1}",
        "ConfigFormatRegex": "^(?<key>[^=]+)=(?<value>.+)$",
        "SettingMappings": {
            "eula=": "EulaAccepted"
        }
    }
]
