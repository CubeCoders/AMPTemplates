[
    {
        "ConfigFile": "server.properties",
        "ConfigType": "kvp",
        "ConfigFormat": "{0}{1}",
        "Importable": "true",
        "Subsections": [
            {
                "Heading": "$root",
                "SettingMappings": {
                    "motd=": "motd",
                    "sub-motd=": "sub-motd",
                    "server-port=": "$ServerPort",
                    "server-ip=": "$ApplicationIPBinding",
                    "view-distance=": "view-distance",
                    "achievements=": "achievements",
                    "announce-player-achievements=": "announce-player-achievements",
                    "spawn-protection=": "spawn-protection",
                    "max-players=": "$MaxUsers",
                    "gamemode=": "gamemode",
                    "force-gamemode=": "force-gamemode",
                    "difficulty=": "difficulty",
                    "hardcore=": "hardcore",
                    "pvp=": "pvp",
                    "white-list=": "white-list",
                    "generator-settings=": "generator-settings",
                    "level-name=": "level-name",
                    "level-seed=": "level-seed",
                    "level-type=": "level-type",
                    "enable-rcon=": "enable-rcon",
                    "rcon.password=": "$RemoteAdminPassword",
                    "force-resources=": "force-resources",
                    "force-resources-allow-client-packs=": "force-resources-allow-client-packs",
                    "xbox-auth=": "xbox-auth",
                    "auto-save=": "auto-save",
                    "force-language=": "force-language",
                    "enable-query=": "enable-query",
                    "allow-flight=": "allow-flight",
                    "allow-nether=": "allow-nether",
                    "allow-the-end=": "allow-the-end",
                    "": "UserSettings"
                }
            }
        ]
    },
    {
        "ConfigFile": "nukkit.yml",
        "ConfigType": "kvp",
        "AutoMap": true,
        "Importable": true,
        "ConfigFormat": " {0}: \"{1}\"",
        "ConfigFormatRegex": "^ (?<key>.+?):\\s*\"(?<value>.*?)\"$"
    }
]