                    "set sv_privateClients":"sv_privateClients",
                    "set sv_allowDownload":"sv_allowDownload",
                    "set sv_fps":"sv_fps",
                    "set sv_minRate":"sv_minRate",
                    "set sv_maxRate":"sv_maxRate",
                    "set sv_minPing":"sv_minPing",
                    "set sv_maxPing":"sv_maxPing",
