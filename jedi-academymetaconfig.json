[
    {
        "ConfigFile":"{{mod}}/base.cfg",
        "ConfigType":"kvp",
        "ConfigFormat":"{0} \"{1}\"",
        "Subsections":[
            {
                "Heading":"$root",
                "SettingMappings":{
                    "//":"This file is controlled by AMP\n// Use server.cfg for any other desired settings",
                    "exec":"server.cfg",
                    "set sv_hostname":"sv_hostname",
                    "set g_motd":"g_motd",
                    "set g_needpass":"g_needpass",
                    "set g_password":"g_password",
                    "rconpassword":"$RemoteAdminPassword",
                    "set sv_privatePassword":"sv_privatePassword",
                    "set sv_pure":"sv_pure",
                    "set sv_cheats":"sv_cheats",
